import Creature from creature
import Brain    from brain
import Body     from body


Population {
	
	.creatures : Creature[]
	
	
	.constructor(n, size) {
		
		[n]() {
			// .creatures();
			.creatures << new Creature(new Brain(size), new Body(size));
		}
		
	}
	
	
	simulate() {
		Creature.simulate();
	}
	
}
